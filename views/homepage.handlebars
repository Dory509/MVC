<section>
    <h1>The Tech Blog</h1>
    {{#each posts}}
        <div class="post">
            <h2>{{this.title}}</h2>
            <p>{{this.content}}</p>
            <p>Posted by {{this.user.username}} on {{this.createdAt}}</p>
            <a href="/post/{{this.id}}">Read more</a>
        </div>
    {{/each}}
</section>